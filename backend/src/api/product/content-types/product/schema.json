{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "default": "sneaker"
    },
    "price": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Unisex",
        "Men",
        "Woman"
      ],
      "default": "Unisex"
    },
    "description": {
      "type": "text",
      "required": true
    }
  }
}
